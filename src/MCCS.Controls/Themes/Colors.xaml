<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="TabSelectedColor">#B00020</Color>
    <Color x:Key="TabNormalColor">#555555</Color>

    <SolidColorBrush x:Key="TabSelectedBrush"
                     Color="{StaticResource TabSelectedColor}"/>

    <SolidColorBrush x:Key="TabNormalBrush"
                     Color="{StaticResource TabNormalColor}"/>
</ResourceDictionary>
