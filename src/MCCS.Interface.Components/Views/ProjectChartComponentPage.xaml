<UserControl x:Class="MCCS.Interface.Components.Views.ProjectChartComponentPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:lv="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             xmlns:viewModels="clr-namespace:MCCS.Interface.Components.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{d:DesignInstance viewModels:ProjectChartComponentPageViewModel}">
    <Grid>
        <lv:CartesianChart 
            AnimationsSpeed="0:0:0"
            Title="{Binding Title}" 
            Series="{Binding CurveSeries}" 
            XAxes="{Binding XAxes}"
            YAxes="{Binding YAxes}" >
        </lv:CartesianChart>
    </Grid>
</UserControl>
