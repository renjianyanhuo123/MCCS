<UserControl x:Class="MCCS.Views.ProjectManager.Components.ProjectChartComponentPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MCCS.Views.ProjectManager.Components"
             xmlns:wpf="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             xmlns:components="clr-namespace:MCCS.ViewModels.ProjectManager.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance components:ProjectChartComponentPageViewModel}">
    <Grid>
        <wpf:CartesianChart 
            AnimationsSpeed="0:0:0"
            Title="{Binding Title}" 
            Series="{Binding CurveSeries}" 
            XAxes="{Binding XAxes}"
            YAxes="{Binding YAxes}" >
        </wpf:CartesianChart>
    </Grid>
</UserControl>
