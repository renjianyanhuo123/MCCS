<UserControl x:Class="MCCS.Views.MethodManager.Contents.MethodInterfaceSettingPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:local="clr-namespace:MCCS.Views.MethodManager.Contents"
             xmlns:layoutRootComponents="clr-namespace:MCCS.Components.LayoutRootComponents"
             xmlns:mccs="clr-namespace:MCCS"
             xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:contents="clr-namespace:MCCS.ViewModels.MethodManager.Contents"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance contents:MethodInterfaceSettingPageViewModel}">
    <b:Interaction.Triggers>
        <b:EventTrigger EventName="Loaded">
            <b:InvokeCommandAction 
                Command="{Binding LoadCommand}" />
        </b:EventTrigger>
    </b:Interaction.Triggers>
    <layoutRootComponents:LayoutRoot DataContext="{Binding LayoutRootViewModel}"></layoutRootComponents:LayoutRoot>
</UserControl>
